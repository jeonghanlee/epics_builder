
EPICS_BUILDER_VARIABLES:=$(sort $(filter-out $(VARS_EXCLUDES) VARS_EXCLUDES,$(.VARIABLES)))

.PHONY : env vars header


## Print interesting VARIABLES
env: vars

vars: header
	$(foreach v, $(EPICS_BUILDER_VARIABLES), $(info $(v) = $($(v)))) @#noop

header:
	$(QUIET)echo ""
	$(QUIET)echo "------------------------------------------------------------"
	$(QUIET)echo ">>>>     EPICS Buliding Envrionment Variables           <<<<"
	$(QUIET)echo "------------------------------------------------------------"
	$(QUIET)echo ""


